: 3px solid #9B8695;\n  margin-top: 5%;\n`;\n\n\nconst StyledButton = styled.button`\nfont-size: 1.42rem;\nborder-radius: 10px;\nbackground-color: #8970A7;\ncolor: white;\nborder: 4px solid ##e1d0f5;\n`;\n\n // ----------- INSTRUCTOR LOGIN COMPONENT -------------\n\nconst InstructorLogin = () => {\n\n    // ----------- INSTRUCTOR LOGIN STATE -------------\n\n    const [instructor, setInstructor] = useState({\n        username: \"\",\n        password: \"\",\n    })\n\n    // ----------- DUMMY POST STATE -------------\n\n    const [post, setPost] = useState([]);\n\n    {/* ------------- BUTTON DISABLED? -------------------- */}\n\n    const [buttonDisabled, setButtonDisabled] = useState(true)\n\n      {/* ------------- ERRORS STATE -------------------- */}\n\n      const [errors, setErrors] = useState({\n          username: \"\",\n          password: \"\",\n      });\n\n    {/* ------------- INPUT CHANGE FUNCTION -------------------- */}\n\n    const inputChange = e => {\n        e.persist();\n        const newUserData = {\n            ...instructor,\n            [e.target.name]: e.target.value\n        };\n\n        validateChange(e);\n        setInstructor(newUserData);\n    }\n\n    {/* ------------- YUP SCHEMA -------------------- */}\n\n    const formSchema = yup.object().shape({\n        username: yup.string().required(\"Username is required.\"),\n        password: yup.string().min(6).max(10).required(\"Must have a valid password between 5 and 10 characters long.\")\n    });\n\n     {/* ------------- USEEFFECT HOOK FOR BUTTON -------------------- */}\n\n     useEffect(() => {\n        console.log('form state change')\n        formSchema.isValid(instructor).then(valid => {\n            console.log('valid?', valid)\n            setButtonDisabled(!valid);\n        });\n\n     }, [instructor]);\n\n\n\n{/* ------------- VALIDATE CHANGE FUNCTION -------------------- */}\n\nconst validateChange = (e) => {\n    yup.reach(formSchema, e.target.name).validate(e.target.value)\n    .then(valid => {\n        setErrors({\n            ...errors, \n            [e.target.name]: \"\"\n        })\n    })\n    .catch(err => {\n        setErrors({\n            ...errors,\n            [e.target.name]: err.errors[0]\n        })\n    })\n}\n\n{/* ------------- FORM SUBMIT FUNCTION -------------------- */}\n{/* ------------- USING DUMMY POST REQUEST -------------------- */}\n\nconst loginSubmit = e => {\n    e.preventDefault();\n    axios\n    .post(\"https://reqres.in/api/users\", instructor)\n    .then(res => {\n        setPost(res.data);\n        console.log(\"success!\", post);\n        // reset form if succesful\n        setInstructor({\n            username: \"\",\n            password: \"\"\n        })\n    })\n    .catch(err => console.log(err.response));\n}\n\n    return (\n        <StyledPage>\n        <div>\n            <StyledH1>üèãÔ∏è instructor login üèãÔ∏è</StyledH1>\n\n        <StyledForm>\n\n        {/* ------------- INSTRUCTOR FORM -------------------- */}\n        <form onSubmit={loginSubmit}>\n\n        <br></br>\n            {/* --------------USERNAME -------------- */}\n            \n            <label htmlFor=\"username\">username:\n            <br></br>\n            <input\n            id=\"username\"\n            type=\"text\"\n            name=\"username\"\n            placeholder=\"please enter username here.\"\n            value={instructor.username}\n            onChange={inputChange}\n            />\n            {errors.username.length > 0 ? <p style={{color: 'red'}}>{errors.username}</p> : null}\n            </label>\n\n            <br></br>\n            <br></br>\n\n             {/* -------------- PASSWORD -------------- */}\n\n             <label htmlFor=\"password\">password:\n             <br></br>\n                <input\n                id=\"password\"\n                type=\"password\"\n                name=\"password\"\n                placeholder=\"password here, please.\"\n                value={instructor.password}\n                onChange={inputChange}\n                />\n                {errors.password.length > 0 ? <p style={{color: 'red'}}>{errors.password}</p> : null}\n             </label>\n\n             <br></br>\n             <br></br>\n\n               {/* -------------- SUBMIT BUTTON -------------- */}\n               {/* <pre>{JSON.stringify(post, null, 2)}</pre> */}\n               <StyledButton disabled={buttonDisabled}>Ready to register?</StyledButton>\n\n        </form>\n        </StyledForm>\n        </div>\n        </StyledPage>\n    )\n}\n\nexport default InstructorLogin","/Users/calebsmith/Lambda2/bw-unit3/front-end-react-/my-app/src/Components/HomeComponents/Nav.js",["49"],"/Users/calebsmith/Lambda2/bw-unit3/front-end-react-/my-app/src/Components/HomeComponents/HomeCards.js",[],"/Users/calebsmith/Lambda2/bw-unit3/front-end-react-/my-app/src/Components/Instructor/InstructorRegister.js",["50","51","52","53","54","55","56","57","58"],"/Users/calebsmith/Lambda2/bw-unit3/front-end-react-/my-app/src/index.js",[],"/Users/calebsmith/Lambda2/bw-unit3/front-end-react-/my-app/src/Components/HomeComponents/Home.js",[],{"ruleId":"59","replacedBy":"60"},{"ruleId":"61","replacedBy":"62"},{"ruleId":"63","severity":1,"message":"64","line":2,"column":8,"nodeType":"65","messageId":"66","endLine":2,"endColumn":12},{"ruleId":"63","severity":1,"message":"67","line":37,"column":9,"nodeType":"65","messageId":"66","endLine":37,"endColumn":16},{"ruleId":"68","severity":1,"message":"69","line":74,"column":5,"nodeType":"70","messageId":"71","endLine":74,"endColumn":64},{"ruleId":"68","severity":1,"message":"69","line":78,"column":7,"nodeType":"70","messageId":"71","endLine":78,"endColumn":62},{"ruleId":"68","severity":1,"message":"69","line":85,"column":5,"nodeType":"70","messageId":"71","endLine":85,"endColumn":69},{"ruleId":"68","severity":1,"message":"69","line":98,"column":5,"nodeType":"70","messageId":"71","endLine":98,"endColumn":58},{"ruleId":"68","severity":1,"m